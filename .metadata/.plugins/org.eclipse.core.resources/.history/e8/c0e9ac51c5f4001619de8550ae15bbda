public class Testing {
	public static void main(String[] args){
		
		int[][] matrix = {
				{1,2,3},
				{4,5,6},
				{7,8,9}
		};
		int[][] primaryStorage = new int[matrix.length -1][matrix.length -1];
		int[][] secondaryStorage = new int[matrix.length - 1][matrix.length -1];
		
		for(int row=0; row<matrix.length; row++){
			System.out.println(" ");
			for(int column=0; column<matrix[row].length; column++){
				System.out.print(matrix[row][column] + ", ");
			}
		}
		for(int i=0; i<matrix.length; i++){
			for(int k=0; k<matrix.length; k++){
			
				primaryStorage[i][k] = matrix[i][k]; 
				
				for(int l=0; l<matrix.length -1; l++){
					
				}
			}

		}
		System.out.println("");
		for(int row=0; row<primaryStorage.length; row++){
			System.out.println(" ");
			for(int column=0; column<primaryStorage.length; column++){
